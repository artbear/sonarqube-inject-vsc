{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "serverId": {
            "type": "string",
            "description": "Unique ID of SonarQube connection from global.json"
        },
        "projectKey": {
            "type": "string",
            "description": "Key of project on SonarQube Server"
        }
    },
    "required": [
        "serverId",
        "projectKey"
    ]
}
